citHeader("To cite package geospt in publications use:")


bibentry(bibtype="Manual",
 title = "geospt: An R package for spatial statistics.",
 author = c(person(given="C.", family="Melo"), person(given="A.", family="Santacruz", email="amsantac@unal.edu.co"), person(given="O.", family="Melo")),
 year = 2025,
 url="https://CRAN.R-project.org/package=geospt", note ="R package version 1.0-5", textVersion = paste("Melo, C., Santacruz, A., Melo, O. 2025. geospt: An R package for spatial statistics.",
 "R package version 1.0-5")
)
